<% layout('dashboard/salonadmin') %>
<div class ="container">
<div class="large-8 columns">
<h class ="align-center">Services</h>
<div class="row collapse" id="appointment">
<a class="small button success left " href="/newservice">Add Service</a>
<table>
  <thead>
    <tr>
      <th>#</th>
      <th>Title</th>
      <th>Description</th>
      <th>Image</th>
      <th>Options</th>
    </tr>
  </thead>
  <tbody>
  <% service.forEach(function(service, index){ %>
    <tr>
    <th scope="row"><%= index + 1 %></th>
      <td><%= service.title  %></td>
      <td><%= service.description  %></td>
      <td>img.jpg</td>
      <td>          
      <ul class="options">
       <li><a href="newservice" title="Edit"><i class="fa fa-pencil-square-o" aria-hidden="true"></i>Update</a></li>
       <li><a href="#" title="Delete"><i class="fa fa-trash" aria-hidden="true"></i>delete</a></li>
      </ul>
      </td>
    </tr>
       <%});%>
  </tbody>
</table>
</div>
</div>
</div>

