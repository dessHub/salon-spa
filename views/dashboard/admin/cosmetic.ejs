<% layout('dashboard/admin') %>
<div class ="container">
<div class="large-8 columns">
<h class ="align-center">Services</h>
<div class="row collapse" id="appointment">
<a class="small button success left " href="/addcosmetic">New</a>
<table class="table">
  <thead>
      <th>#</th>
      <th>Salon Name</th>
      <th>Location</th>
      <th>Hours</th>
      <th>Description</th>
      <th>Posts</th>
      <th>Contacts</th>
      <th>Options</th>
  </thead>
  <tbody>
  <% cosmetic.forEach(function(cosmetic, index){ %>
    <tr>
    <th scope="row"><%= index + 1 %></th>
      <td><%= cosmetic.name  %></td>
      <td><%= cosmetic.location  %></td>
      <td><%= cosmetic.hours  %></td>
      <td><%= cosmetic.description %></td>
      <td>10</td>
      <td><%= cosmetic.contact %></td>
      <td><a href="/cosmeticdelete/<%= cosmetic._id %>" title="Delete"><i class="fa fa-trash" aria-hidden="true"></i>delete</a>
      </td>
    </tr>
       <%});%>
  </tbody>
</table>


</div>
</div>
</div>